{
  "name": "monocot",
  "version": "0.2.0",
  "description": "Seed project for an ES6 MEAN site",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielahedges/monocot.git"
  },
  "scripts": {
    "build": "npx babel src -s -d build",
    "start": "node build/server.js",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "watch": "npx babel src -s -d build --watch",
    "test": "npx mocha build/**/*.spec.js",
    "test:staging": "ls",
    "precommit": "npm run lint:fix && npm run test",
    "prepush": "make ci"
  },
  "keywords": ["MongoDB", "Express", "Angular", "Node"],
  "author": "Dan Hedges",
  "license": "MIT",
  "dependencies": {
    "body-parser": "~1.18.3",
    "compression": "~1.7.2",
    "connect-flash": "~0.1.1",
    "express": "~4.16.3",
    "express-session": "~1.15.6",
    "lodash": "~4.17.10",
    "method-override": "~2.3.10",
    "mongoose": "~5.1.2",
    "morgan": "~1.9.0",
    "passport": "~0.4.0",
    "passport-facebook": "~2.1.1",
    "passport-google-oauth": "~1.0.0",
    "passport-local": "~1.0.0",
    "passport-twitter": "~1.0.4",
    "pug": "~2.0.3",
    "source-map-support": "~0.5.6"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-preset-env": "~1.7.0",
    "chai": "~4.1.2",
    "eslint": "~4.19.1",
    "eslint-config-standard": "~11.0.0",
    "eslint-plugin-import": "~2.12.0",
    "eslint-plugin-node": "~6.0.1",
    "eslint-plugin-promise": "~3.7.0",
    "eslint-plugin-standard": "~3.1.0",
    "husky": "~0.14.3",
    "mocha": "~5.2.0",
    "mockery": "~2.1.0",
    "sinon": "~5.0.10"
  }
}
